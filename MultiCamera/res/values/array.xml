<!-- Array resources for the Phone app. -->
<resources>
    <!-- "Target" resources for the GlowPadView widget used for incoming calls;
         see InCallTouchUi.showIncomingCallWidget() and incall_touch_ui.xml.  -->

    <!-- For most incoming calls the GlowPadView widget provides 3 choices:
         - Answer (drag right)
         - Reject (drag left)
         - Respond via SMS (drag up) -->

    <string-array name="multicamera_list_videosize">
        <item>@string/size640_480</item>
        <item>@string/size640_360</item>
        <item>@string/size544_288</item>
        <item>@string/size432_240</item>
        <item>@string/size352_288</item>
        <item>@string/size320_240</item>
        <item>@string/size320_176</item>
        <item>@string/size160_120</item>
    </string-array>

    <string-array name="multicamera_list_framerate">
        <item>@string/frame15</item>
        <item>@string/frame30</item>
    </string-array>

<!-- enable the settings if supportConfigure codecprofile
    <string-array name="multicamera_list_codecprofile">
		<item>@string/QUALITY_QVGA</item>  
		<item>@string/QUALITY_480P</item> 
    </string-array>
    <string-array name="multicamera_list_codecprofile_no">
		<item>@string/QUALITY_QVGA_VAL</item>  
		<item>@string/QUALITY_480P_VAL</item> 
    </string-array>
-->

